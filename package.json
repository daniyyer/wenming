  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "prod:nohash": "npm run webpack -- --env.mode production --env.isHash false",
    "prod": "npm run webpack -- --env.mode production",
    "dev": "npm run webpack-dev-server -- --env.mode development   ",
    "dev:nohash": "npm run webpack-dev-server -- --env.mode development  --env.isHash false ",
    "kill": "kill-port 9000",
    "stop-port": "netstat -aon | find /i \"listening\" | find \":9000\" >nul  && (for /f \"tokens=5\" %a in ('netstat -aon ^| find \":9000\" ^| find \"LISTENING\"') do  taskkill /f /pid %a) || (echo 2) ",
    "dev:nodemon": "nodemon --watch webpack.config.js --exec \"webpack-dev-server  --env.mode development\"",
    "dev:watch": " chokidar \"src/layouts\" --depth \"2\" --watch-ready \"true\" -i \"src/layouts/**/*.*\" -c \"kill-port 9000 && npm run dev\" ",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --config .prettierrc.js  --write \"src/**/*.{js,jsx,tsx,ts,less,md,json,html,ejs}\" ",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx  ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx  ./src",
    "lint:style": "stylelint --fix \"src/**/*.scss\" ",
    "prettier": "prettier -c --config .prettierrc.js   --write \"**/*.js\"",
    "postinstall": "patch-package"
  },
  "browserslist": [
    "defaults",
    "IE 10",
    "maintained node versions"
  ],
  "postcss": {
    "map": false,
    "plugins": {}
